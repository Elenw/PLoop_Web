@{ inherit="Share/MasterPage", unique = true, cache = true }

@title{
	@self.PageTitle
}

@jspart{
	@{javascript("_static/jquery")}
}

@body{
	<p>
		@[Description Need a description]
	</p>
	<p>100 + 111 = @{plus(100, 111)}</p>
	<div>
		@{ table(self.Data, {"Name", "Person Name", "Age", "Person Age"}) }
	</div>
}